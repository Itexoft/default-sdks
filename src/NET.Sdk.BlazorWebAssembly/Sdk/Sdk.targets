    <Import Project="Global.targets"/>

    <ItemGroup>
        <PackageReference Include="Itexoft.dotnet.runtime.wasm" Version="$([MSBuild]::GetTargetFrameworkVersion('$(TargetFrameworkVersion)', 2)).*" PrivateAssets="all" ExcludeAssets="runtime;native" GeneratePathProperty="true"/>
