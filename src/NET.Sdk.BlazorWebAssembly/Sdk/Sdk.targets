    <Import Project="Sdk.targets" Sdk="Microsoft.NET.Sdk.BlazorWebAssembly"/>
    <Import Project="Wasm.targets" Condition="$([System.String]::Copy('$(TargetFramework)').Contains('-browser'))"/>
